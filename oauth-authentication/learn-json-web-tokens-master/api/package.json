    "fullstack-validator": "^1.0.0",
    "ip": "1.1.8",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "^2.29.3",
    "mongoose": "6.3.3",
    "morgan": "1.10.0",
    "multer": "1.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
